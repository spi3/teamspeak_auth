# TeamSpeak ServerQuery Connection Settings
TS_HOST=localhost
TS_PORT=10011
TS_USER=serveradmin
TS_PASSWORD=your_password_here
TS_SERVER_ID=1

# Required server group IDs (comma-separated)
# Users with any of these groups are considered authorized
REQUIRED_SERVER_GROUPS=6,9

# FastAPI Server Settings
API_HOST=0.0.0.0
API_PORT=8000

# Cache TTL (seconds)
# How often to refresh the list of authorized users from TeamSpeak
CACHE_TTL=30
